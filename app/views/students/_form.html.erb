<%= form_for @student do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :mod_name %>
  <%= f.text_field :mod_name %><br>
  <%= f.label :mod_start_date %>
  <%= f.number_field :mod_start_date %><br>
  <%= f.label :github_link %>
  <%= f.url_field :github_link %><br>
  <%= f.label :linkedin_link %>
  <%= f.url_field :linkedin_link %><br>
  <%= f.submit %>
<% end %>
