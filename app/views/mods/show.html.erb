
<h1> Mod <%= @mod.mod_number %>'s Notes </h1>

<div class="navigation-links">
<%= link_to "View All Notes", notes_path %>
</div>
<br>
<ul>
  <% @mod.notes.each do |note|%>
    <% if note.content %>
      <li><%= link_to note.lecture_name, note_path(note) %></li><br>
    <% end %>
  <% end %>

  </ul>
<br>
<div class="page-links">

<%= link_to "Go to Instructors", instructors_path %>
<%= link_to "Go to Students", students_path %>
<%= link_to "Go to Resource", resources_path %>
</div>

<br>
<div class="navigation-links">
<%= link_to "Back to Mods", mods_path %>
</div>
