<%= form_for @note do |f| %>
<%= f.label "Mod:" %>
<%= f.collection_select :mod, Mod.all, :id, :mod_description %>
<%= f.label "Lecture Name:" %>
<%= f.text_field :lecture_name %> <br> <br>

<%= f.text_area :content, placeholder: "Paste your notes here", size: "60x12"  %> <br>

<%= f.label "Highlights" %><br>
<%= f.text_field :highlights %><br>
<%= f.submit "save my notes"%>

<% end %>
